{#
/**
 * @file
 * The views infinite scroll pager template.
 */
#}

{% if items.next %}
<ul{{ attributes }}>
  <li class="pager__item">
    <a class="button btn-readmore" href="{{ items.next.href }}" title="{{ 'Load more items'|t }}" rel="next"><i class="fa fa-angle-down" style="font-size:36px"></i></a>
  </li>
</ul>
{% endif %}
